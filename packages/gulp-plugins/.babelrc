{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "12"
        },
        "shippedProposals": true
      }
    ]
  ],
  "plugins": ["source-map-support", "@babel/plugin-transform-runtime"],
  "comments": false,
  "sourceMaps": "both",
  "env": {
    "coverage": {
      "plugins": [
        [
          "istanbul",
          {
            "exclude": ["test", "build"]
          }
        ]
      ]
    }
  }
}
