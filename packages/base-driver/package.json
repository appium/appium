{
  "name": "@appium/base-driver",
  "version": "8.0.0-beta.6",
  "description": "Base driver class for Appium drivers",
  "keywords": [
    "appium"
  ],
  "bugs": {
    "url": "https://github.com/appium/appium/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium.git"
  },
  "license": "Apache-2.0",
  "author": "appium",
  "type": "module",
  "exports": {
    ".": {   
      "import": "./index.js",
      "require": "./build/index.js",
      "default": "./index.js"
    },
