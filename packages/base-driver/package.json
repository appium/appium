{
  "name": "@appium/base-driver",
  "version": "8.5.7",
  "description": "Base driver class for Appium drivers",
  "keywords": [
    "automation",
    "javascript",
    "selenium",
    "webdriver",
    "ios",
    "android",
    "firefoxos",
    "testing"
  ],
  "homepage": "https://appium.io",
  "bugs": {
    "url": "https://github.com/appium/appium/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium.git",
    "directory": "packages/base-driver"
  },
  "license": "Apache-2.0",
  "author": "https://github.com/appium",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "static",
    "test/basedriver",
    "!test/e2e/fixtures",
    "build",
    "!build/test/e2e/fixtures"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:distfiles": "babel lib --root-mode=upward --out-dir=build/lib",
    "build:test": "babel test --root-mode=upward --out-dir=build/test --copy-files",
    "dev": "run-p \"build:distfiles -- --watch\" \"build:test -- --watch\"",
    "fix": "npm run lint -- --fix",
    "lint": "eslint -c ../../.eslintrc --ignore-path ../../.eslintignore .",
    "prepare": "npm run build",
    "test": "npm run test:unit",
    "test:e2e": "mocha --timeout 20s --slow 10s \"./test/e2e/**/*.spec.js\"",
    "test:smoke": "node ./index.js",
    "test:unit": "mocha \"./test/unit/**/*.spec.js\""
  },
  "dependencies": {
    "@appium/support": "^2.59.2",
    "@babel/runtime": "7.18.3",
    "@colors/colors": "1.5.0",
    "async-lock": "1.3.2",
    "asyncbox": "2.9.2",
    "axios": "0.27.2",
    "bluebird": "3.7.2",
    "body-parser": "1.20.0",
    "es6-error": "4.1.1",
    "express": "4.18.1",
    "http-status-codes": "2.2.0",
    "lodash": "4.17.21",
    "lru-cache": "7.10.2",
    "method-override": "3.0.0",
    "morgan": "1.10.0",
    "serve-favicon": "2.5.0",
    "source-map-support": "0.5.21",
    "validate.js": "0.13.1"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./build/lib/index.d.ts",
  "gitHead": "5c7af8ee73078018e4ec52fccf19fe3f77249d72"
}
