{
  "name": "appium",
  "version": "2.0.0-beta.33",
  "description": "Automation for Apps.",
  "keywords": [
    "automation",
    "javascript",
    "selenium",
    "webdriver",
    "ios",
    "android",
    "firefoxos",
    "testing"
  ],
  "homepage": "https://appium.io",
  "bugs": {
    "url": "https://github.com/appium/appium/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium.git",
    "directory": "packages/appium"
  },
  "license": "Apache-2.0",
  "author": "https://github.com/appium",
  "bin": {
    "appium": "index.js"
  },
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib",
    "build",
    "index.js",
    "scripts/postinstall.js",
    "types"
  ],
  "scripts": {
    "build": "babel lib --root-mode=upward --out-dir=build/lib",
    "dev": "npm run build -- --watch",
    "fix": "npm run lint -- --fix",
    "build:docs": "node docs/scripts/build-docs.js",
    "build:docs:assets": "node docs/scripts/copy-assets.js",
    "dev:docs": "npm run build:docs:assets && npm run dev:docs:en",
    "dev:docs:en": "mkdocs serve -f ./docs/mkdocs-en.yml",
    "dev:docs:ja": "mkdocs serve -f ./docs/mkdocs-ja.yml",
    "publish:docs": "APPIUM_DOCS_PUBLISH=1 npm run build:docs",
    "postinstall": "node ./scripts/postinstall.js",
    "lint": "eslint -c ../../.eslintrc --ignore-path ../../.eslintignore .",
    "test": "npm run test:unit",
    "test:e2e": "mocha --timeout 30s --slow 15s \"./test/e2e/**/*.spec.js\"",
    "test:unit": "mocha \"./test/unit/**/*.spec.js\""
  },
  "dependencies": {
    "@appium/base-driver": "^8.5.2",
    "@appium/base-plugin": "^1.8.4",
    "@appium/docutils": "^0.0.3",
    "@appium/schema": "^0.0.5",
    "@appium/support": "^2.57.4",
    "@babel/runtime": "7.17.9",
    "@sidvind/better-ajv-errors": "1.1.1",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "argparse": "2.0.1",
    "async-lock": "1.3.1",
    "asyncbox": "2.9.2",
    "axios": "0.26.1",
    "bluebird": "3.7.2",
    "find-up": "5.0.0",
    "lilconfig": "2.0.5",
    "lodash": "4.17.21",
    "longjohn": "0.2.12",
    "npmlog": "6.0.2",
    "ora": "5.4.1",
    "package-changed": "1.9.0",
    "resolve-from": "5.0.0",
    "semver": "7.3.7",
    "source-map-support": "0.5.21",
    "teen_process": "1.16.0",
    "winston": "3.7.2",
    "word-wrap": "1.2.3",
    "yaml": "2.0.1"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "types": "./build/lib/main.d.ts",
  "gitHead": "722ae145fb40bf17b0168ea8b025763f120ad574"
}
