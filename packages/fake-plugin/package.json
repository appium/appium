{
  "name": "@appium/fake-plugin",
  "version": "2.1.3",
  "description": "A fake Appium 2.0 plugin",
  "homepage": "https://appium.io",
  "bugs": {
    "url": "https://github.com/appium/appium/issues"
  },
  "repository": {
    "type": "git",
