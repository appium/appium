  "dependencies": {
    "@appium/support": "^2.52.0",
    "@babel/runtime": "^7.0.0",
    "async-lock": "^1.0.0",
    "asyncbox": "^2.3.1",
    "axios": "^0.x",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "es6-error": "^4.1.1",
    "express": "^4.16.2",
    "http-status-codes": "^2.1.4",
