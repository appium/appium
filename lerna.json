{
  "command": {
    "exec": {
      "ignore": ["@appium/eslint-config-appium", "appium"]
    },
    "run": {
      "prefix": false,
      "concurrency": 1,
      "stream": true
    },
    "version": {
      "conventionalCommits": true,
      "createRelease": "github"
    },
    "add": {
      "exact": true
    }
  },
  "ignoreChanges": [
    "**/test/**",
    "**/*.md"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
