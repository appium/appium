{
  "ci": false,
  "command": {
    "exec": {
      "ignore": "@appium/eslint-config-appium"
    },
    "version": {
      "conventionalCommits": true,
      "createRelease": true
    }
  },
  "ignoreChanges": [
    "**/test/**",
    "**/*.md"
  ],
  "npmClientArgs": [
    "--no-package-lock"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
